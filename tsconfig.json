{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@img/*": ["./img/*"],
    }
  },
  "include": [
    "./src/*", "images.d.ts"
  ],
  "exclude": [
    "./node_modules"
  ]
}